-- Scripts:

local function UDNY_fake_script() -- Execute.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Execute

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.event:FireServer(	script.Parent.Parent.Parent.Executing.ScrollingFrame.TextBox.Text)
		print(script.Parent.Parent.Parent.Executing.ScrollingFrame.TextBox.Text)	
	end)
end
coroutine.wrap(UDNY_fake_script)()
local function KHAWTDX_fake_script() -- Execute.Script 
	local script = Instance.new('Script', Execute)
	script.Parent = Execute

	script.Parent.event.OnServerEvent:Connect(function(p,strin)
		require(script.vLua)(strin)()
	end)
end
local event = Instance.new('RemoteEvent')
event.Name = "event"
event.Parent = Execute
coroutine.wrap(KHAWTDX_fake_script)()
local function MKFF_fake_script() -- Clear.LocalScript 
