    script.Parent = Clear
	script.Parent.MouseButton1Click:Connect(function() 
		script.Parent.Parent.Parent.Executing.ScrollingFrame.TextBox.Text = ""
	end)
end
coroutine.wrap(MKFF_fake_script)()
local function OJEPZ_fake_script() -- Hint.LocalScript 
	local script = Instance.new('LocalScript')
    script.Parent = Hint
	script.Parent.MouseButton1Click:Connect(function() 
		local hint = Instance.new("Hint")
		hint.Parent = game.Workspace
		hint.Name = "GET HACKED"
		hint.Text = "TEAM FER MODZ, JOIN NOW!!!"
	end)
end
coroutine.wrap(OJEPZ_fake_script)()
local function ODTZ_fake_script() -- Kill.LocalScript 
	local script = Instance.new('LocalScript')
    script.Parent = Kill
	local textButton = script.Parent  -- Assuming the script is a child of the TextButton
	
	-- Function to check the text and set other players' health to 0
	local function onClick()
		local player = game.Players.LocalPlayer  -- Get the player who clicked the button
		local players = game.Players:GetPlayers()  -- Get a list of all players
	
		-- Iterate through all players
		for _, otherPlayer in pairs(players) do
			if otherPlayer ~= player then
				-- Check if the player has a character and humanoid
				local character = otherPlayer.Character
				local humanoid = character and character:FindFirstChild("Humanoid")
	
				if humanoid then
					humanoid.Health = 0
					print(otherPlayer.Name .. "'s humanoid health set to 0.")
				else
					print(otherPlayer.Name .. " does not have a humanoid.")
				end
			end
		end
	end
	
	-- Bind the function to the TextButton's Click event
	textButton.MouseButton1Click:Connect(onClick)
end
coroutine.wrap(ODTZ_fake_script)()
local function SRVE_fake_script() -- button.LocalScript 
	local script = Instance.new('LocalScript')
    script.Parent = button
	script.Parent.MouseButton1Click:Connect(function() 
		script.Parent.Parent.console.Visible = true
		script.Parent.Parent.Close.Visible = true
	end)
	
end
local Log = Instance.new('RemoteEvent')
Log.Name = "LogAdded"
Log.Parent = console
coroutine.wrap(SRVE_fake_script)()
local function WGVXEWH_fake_script() -- console.Script 
	local script = Instance.new('Script')
    script.Parent = console
	local ls = game:GetService('LogService')
	local colors = {[0] = Color3.new(1, 1, 1), Color3.new(0/255,98/255,255/255), Color3.new(1, 1, 0), Color3.new(1, 0, 0)}
	local num = 0
	ls.MessageOut:Connect(function(msg,type)
		script.Parent.LogAdded:FireClient(script.Parent.Parent.Parent.Parent.Parent.Parent)
		num = num + 1
		local t = tick()
		local l = script.Parent.output:Clone()
		l.Position = UDim2.new(0, 0, 0, num*24)
		l.Text = ('>[%02i:%02i:%02i]   %s  -  SS'):format((t/3600)%24, (t/60)%60, t%60, msg)
		l.TextColor3 = colors[type.Value]
		l.Parent = script.Parent
	end)
	script.Parent.LogAdded.OnServerEvent:Connect(function(Player)
		if Player == script.Parent.Parent.Parent.Parent then
			num += 1
		end
	end)
	
end
coroutine.wrap(WGVXEWH_fake_script)()
local function LTGSHB_fake_script() -- console.LocalScript 
	local script = Instance.new('LocalScript')
    script.Parent = console
	local ls = game:GetService('LogService')
	local colors = {[0] = Color3.new(1, 1, 1), Color3.new(0/255,98/255,255/255), Color3.new(1, 1, 0), Color3.new(1, 0, 0)}
	local num = 0
	ls.MessageOut:Connect(function(msg,type)
		script.Parent.LogAdded:FireServer()
		num = num + 1
		local t = tick()
		local l = script.Parent.output:Clone()
		l.Position = UDim2.new(0, 0, 0, num*24)
		l.Text = ('>[%02i:%02i:%02i]   %s  -  Server'):format((t/3600)%24, (t/60)%60, t%60, msg)
		l.TextColor3 = colors[type.Value]
		l.Parent = script.Parent
	end)
	script.Parent.LogAdded.OnClientEvent:Connect(function(Player)
		if Player == script.Parent.Parent.Parent.Parent then
			num += 1
		end
	end)
end
coroutine.wrap(LTGSHB_fake_script)()
local function WPBQDL_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript')
    script.Parent = Close
	script.Parent.MouseButton1Click:Connect(function() 
		script.Parent.Parent.console.Visible = false
		wait(0.00000000000000000000000000000000000000000000000001)
		script.Parent.Visible = false
	end)
	
end
coroutine.wrap(WPBQDL_fake_script)()
